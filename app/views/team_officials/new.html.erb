<section role="main" class="content-body">
  <header class="page-header">
    <h2>Data Pegawai Pasukan</h2>
    <div class="right-wrapper pull-right">
      <ol class="breadcrumbs">
        <li>
          <%= link_to root_path do %>
            <i class="fa fa-home"></i>
          <% end %>
        </li>
        <li>
          <span>Data Pegawai Pasukan</span>
        </li>
      </ol>
      <a class="sidebar-right-toggle" data-open="sidebar-right">
        <i class="fa fa-chevron-left"></i>
      </a>
    </div>
  </header>
  <div class="row">
    <div class="col-md-12">
      <section class="panel">
        <header class="panel-heading">
          <h2 class="panel-title">Data Pegawai Pasukan</h2>
          <p class="text-danger">Minimum pegawai pasukan mestilah tidak kurang dari 5 orang.</p>
        </header>
        <div class="panel-body">
          <!-- Main Content Start -->
          <%= form_for @team_official, url: team_team_officials_path, multipart: true, html: { class: "form-horizontal form-bordered", method: :post } do |f| %>
            <%= render partial: 'form', locals: {f: f, post: @team_official} %>
          <% end %>
          <!-- Main Content End -->
        </div>
      </section>
    </div>
  </div>
</section>
