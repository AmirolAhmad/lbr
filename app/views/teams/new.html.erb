<section role="main" class="content-body">
  <header class="page-header">
    <h2>My Team</h2>
    <div class="right-wrapper pull-right">
      <ol class="breadcrumbs">
        <li>
          <%= link_to root_path do %>
            <i class="fa fa-home"></i>
          <% end %>
        </li>
        <li>
          <%= link_to team_path do %>
            <span>My Team</span>
          <% end %>
        </li>
        <li>
          <%= link_to new_team_path do %>
            <span>Create Team</span>
          <% end %>
        </li>
      </ol>
      <a class="sidebar-right-toggle" data-open="sidebar-right">
        <i class="fa fa-chevron-left"></i>
      </a>
    </div>
  </header>
  <div class="row">
    <div class="col-md-12">
      <section class="panel">
        <header class="panel-heading">
          <h2 class="panel-title">Create Team</h2>
        </header>
        <div class="panel-body">
          <!-- Main Content Start -->
          <%= form_for @team, url: team_url, multipart: true, html: { class: "form-horizontal form-bordered", method: :post } do |f| %>
            <%= render partial: 'form', locals: {f: f, post: @team} %>
          <% end %>
          <!-- Main Content End -->
        </div>
      </section>
    </div>
  </div>
</section>
