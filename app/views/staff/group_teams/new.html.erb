<section role="main" class="content-body">
  <header class="page-header">
    <h2>Daftar Pasukan</h2>
    <div class="right-wrapper pull-right">
      <ol class="breadcrumbs">
        <li>
          <%= link_to root_path do %>
            <i class="fa fa-home"></i>
          <% end %>
        </li>
        <li>
          <span>Daftar Pasukan</span>
        </li>
      </ol>
      <a class="sidebar-right-toggle" data-open="sidebar-right">
        <i class="fa fa-chevron-left"></i>
      </a>
    </div>
  </header>
  <div class="row">
    <div class="col-md-12">
      <section class="panel">
        <header class="panel-heading">
          <h2 class="panel-title">Daftar Pasukan Baru bagi <%= @group.name %></h2>
        </header>
        <div class="panel-body">
          <!-- Main Content Start -->
          <%= form_for @staff_group_team, url: staff_zone_group_group_teams_path, multipart: true, html: { class: "form-horizontal form-bordered", method: :post } do |f| %>
            <%= render partial: 'form', locals: {f: f, post: @staff_group_team} %>
          <% end %>
          <!-- Main Content End -->
        </div>
      </section>
    </div>
  </div>
</section>
